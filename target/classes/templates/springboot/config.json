{
  "vars": {
    "basePackage": "com.${company}.${projectName}",
    "mapperPackage": "com.${company}.${projectName}.db.mapper",
    "modelPackage": "com.${company}.${projectName}.db.model",
    "servicePackage": "com.${company}.${projectName}.service",
    "serviceImplPackage": "com.${company}.${projectName}.service.impl",
    "controllerPackage": "com.${company}.${projectName}.controller",
    "commonPackage": "com.${company}.${projectName}.common",
    "voPackage": "com.${company}.${projectName}.service.vo"
  },
  "globalTemplates": [{
    "name": "BaseService.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/service/Service.java"
  },{
    "name": "BaseServiceImpl.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/service/impl/ServiceImpl.java"
  },{
    "name": "BaseMapper.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/db/mapper/Mapper.java"
  },{
    "name": "CommonResult.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/common/CommonResult.java"
  },{
    "name": "CommonResponse.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/common/CommonResponse.java"
  },{
    "name": "CustomizeCode.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/common/CustomizeCode.java"
  },{
    "name": "Maven.flt",
    "output": "${outputPath}/pom.xml"
  },{
    "name": "Main.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/SpringbootCliApplication.java"
  },,{
    "name": "AppConfig.flt",
    "output": "${outputPath}/src/main/resources/application.yml"
  }],
  "tableTemplates": [{
    "name": "Service.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/service/${r"${table.name}"}Service.java"
  },{
    "name": "ServiceImpl.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/service/impl/${r"${table.name}"}ServiceImpl.java"
  },{
    "name": "Vo.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/service/vo/${r"${table.name}"}VO.java"
  },{
    "name": "Model.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/db/model/${r"${table.name}"}.java"
  },{
    "name": "Mapper.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/db/mapper/${r"${table.name}"}Mapper.java"
  },{
    "name": "MapperXml.flt",
    "output": "${outputPath}/src/main/resources/com/${company}/${projectName}/db/mapper/${r"${table.name}"}.xml"
  },{
    "name": "Controller.flt",
    "output": "${outputPath}/src/main/java/com/${company}/${projectName}/controller/${r"${table.name}"}Controller.java"
  }]
}